#include <stdio.h>
int main(){
    char a[100];
    int count=0;
    for(int i=0;i<100;i++){
        a[i]='\0';
    }
    scanf("%s",a);
    for(int k=0;k<100;k++){
        if(a[k]!='\0'){
            count++;        
        }
        
    }
    
    for(int i=count-1;i>=0;i--){
        printf("%c",a[i]);
    }
    printf("\n");
    return 0;
}
