#include <stdio.h>
void print_sp(int i, int n){
    for(int sp=0;sp<n-i;sp++){
        printf(" ");
    }
}
void print_num(int n){
    for(int i=0; i<n;i++){
        printf("%d" , n);
        printf(" ");
    }
    printf("\n");
    
}
int main(){
    int row=6;
    for(int i=0; i<=row;i++){
        print_sp(i, row);
        print_num(i);
    }
    return 0;
}
